#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gongxun/Documents/goduer/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/gongxun/Documents/goduer/monorepo/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@tarojs+cli@3.6.2_postcss@8.4.18+vue@3.2.47/node_modules/@tarojs/cli/bin/taro" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@tarojs+cli@3.6.2_postcss@8.4.18+vue@3.2.47/node_modules/@tarojs/cli/bin/taro" "$@"
fi
